{
  "name": "boilerplate-react-webpack",
  "version": "0.2.2",
  "description": "A webpack React starter",
  "repository": "https://github.com/dev-academy-challenges/boilerplate-react-webpack.git",
  "main": "server/index.js",
  "author": "EDA",
  "license": "ISC",
  "scripts": {
    "dev": "run-p dev:server dev:client",
    "dev:server": "nodemon .",
    "dev:client": "webpack --config webpack.config.js --watch",
    "client": "webpack-dev-server --hot --inline",
    "build": "webpack --config webpack.config.js",
    "knex": "knex --knexfile knexfile.js",
    "db:reset": "run-s db:delete db:migrate db:seed",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "db:delete": "rm -f dev.sqlite3",
    "server": "node server",
    "start": "webpack && node server",
    "test": "jest --watchAll"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "authenticare": "^0.4.5",
    "babel-polyfill": "^6.26.0",
    "enzyme-to-json": "^3.4.3",
    "express": "^4.16.4",
    "knex": "^0.20.3",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "react-countdown-now": "^2.1.2",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "save": "^2.4.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1",
    "sqlite3": "^4.1.1",
    "styled-components": "^4.4.1",
    "superagent": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.7.1",
    "react": "^16.8.6",
    "react-datepicker": "^2.10.1",
    "react-dom": "^16.8.6",
    "supertest": "^4.0.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
